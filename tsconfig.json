{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      "@kitn/core": ["./_stubs/core.d.ts"],
      "@kitn/agents/*": ["./_staging/agents/*"],
      "@kitn/tools/*": ["./_staging/tools/*"],
      "@kitn/skills/*": ["./_staging/skills/*"],
      "@kitn/storage/*": ["./_staging/storage/*"]
    }
  },
  "include": ["_staging/**/*.ts"]
}
